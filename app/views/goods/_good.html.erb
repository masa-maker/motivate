<div class="like_button" id="likes_buttons_<%= post.id %>">
  <% if post.liked_by?(current_user) %>
    <%= link_to "いいね", post_good_path(post.id), method: :delete, remote: true, class:"btn-sm btn-outline-danger"%>
  <% else %>
    <%= link_to "いいね", post_goods_path(post.id), method: :post, remote: true, class:"btn-sm btn-danger"%>
  <% end %>
</div>